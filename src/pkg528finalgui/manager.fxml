<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pkg528finalgui.ManagerGUIController">
   <children>
      <GridPane layoutY="-1.0" prefHeight="400.0" prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="logoutBtn" mnemonicParsing="false" onAction="#handleLogout" text="Logout" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="6">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font></Button>
            <Button fx:id="deleteBtn" mnemonicParsing="false" onAction="#handleDelete" text="Delete" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="5" />
            <Button fx:id="addBtn" mnemonicParsing="false" onAction="#handleAdd" text="Add" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="3" />
            <TextField fx:id="username1Txt" promptText="Username" GridPane.rowIndex="2" />
            <Label text="Add new Customer:" GridPane.rowIndex="1" />
            <TextField fx:id="username2Txt" promptText="Username" GridPane.rowIndex="5" />
            <Label text="Delete Customer:" GridPane.rowIndex="4" />
            <Label text="Manager Account">
               <font>
                  <Font name="System Bold" size="34.0" />
               </font>
            </Label>
            <PasswordField fx:id="passwordTxt" prefHeight="39.0" prefWidth="73.0" promptText="Password" GridPane.rowIndex="3" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
